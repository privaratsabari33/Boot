DROP TABLE IF EXISTS TBL_FLIGHT;
  
CREATE TABLE FLIGHT (
  id LONG AUTO_INCREMENT  PRIMARY KEY,
  flightname VARCHAR(250) NOT NULL,
  source VARCHAR(250) NOT NULL,
  destination VARCHAR(250) NOT NULL,
  date VARCHAR(250) NOT NULL,
  departure VARCHAR(250) NOT NULL,
  arrival VARCHAR(250) NOT NULL,
  departureDate VARCHAR(250) NOT NULL,
  arrivalDate VARCHAR(250) NOT NULL,
);

INSERT INTO FLIGHT (id, flightname, source,destination,date,departure,arrival,departureDate,arrivalDate) VALUES
  (1, 'AirIndia', 'Hyderabad','Chennai', '2020-03-20','D','A','2020-03-20','2020-03-20','2020-03-21');

  	
  	
DROP TABLE IF EXISTS TBL_PASSENGERDETAILS;
  
CREATE TABLE PASSENGERDETAILS (
  id LONG AUTO_INCREMENT  PRIMARY KEY,
  name VARCHAR(250) NOT NULL,
  gender VARCHAR(250) NOT NULL,
  age VARCHAR(250) NOT NULL,
  email VARCHAR(250) NOT NULL,
);

INSERT INTO PASSENGERDETAILS (id,name, gender,age,email) VALUES
  (1, 'Reeta', 'F','21', 'abc@gmail.com');
INSERT INTO PASSENGERDETAILS (id,name, gender,age,email) VALUES
  (2, 'Siva', 'M','35', 'siva@gmail.com');
  	
  	
  	
  	
  	
  	
  
  	